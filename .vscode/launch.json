{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug CalDAV Tests",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/caldav",
            "args": [
                "-test.v"
            ],
            "env": {},
            "showLog": true,
            "trace": "verbose"
        },
        {
            "name": "Debug Apple Sync Test",
            "type": "go",
            "request": "launch",
            "mode": "test",
